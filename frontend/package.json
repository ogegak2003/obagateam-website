{
  "name": "obagateam-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "echo 'No tests yet' && exit 0",
    "test:ci": "npm test",
    "build:docker": "docker build -t $npm_package_name -f Dockerfile.prod .",
    "build:docker:dev": "docker build -t $npm_package_name:dev -f Dockerfile .",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "production": "npm run build && npm run preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "framer-motion": "^10.16.1",
    "react-hook-form": "^7.45.4",
    "socket.io-client": "^4.7.2",
    "react-leaflet": "^4.2.1",
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  },
  "keywords": [
    "react",
    "vite",
    "obagaTeam",
    "frontend",
    "website"
  ],
  "author": "obagaTeam",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}